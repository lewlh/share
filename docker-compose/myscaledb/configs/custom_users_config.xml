<?xml version="1.0"?>
<clickhouse>
    <profiles>
        <default>
            <allow_experimental_database_replicated>true</allow_experimental_database_replicated>
            <database_replicated_always_convert_table_to_replicated>true</database_replicated_always_convert_table_to_replicated>
            <database_replicated_allow_replicated_engine_arguments>false</database_replicated_allow_replicated_engine_arguments>
            <database_replicated_default_zk_path_prefix>/clickhouse/moqi/databases/</database_replicated_default_zk_path_prefix>
            <database_replicated_always_execute_with_on_cluster>true</database_replicated_always_execute_with_on_cluster>
            <database_replicated_default_cluster_name>moqi</database_replicated_default_cluster_name>
            <database_replicated_allow_explicit_arguments>false</database_replicated_allow_explicit_arguments>
            <default_database_engine>Replicated</default_database_engine>
            <default_table_engine>ReplicatedMergeTree</default_table_engine>
            <max_memory_usage>4294967296</max_memory_usage>
            <load_balancing>random</load_balancing>
            <log_queries>1</log_queries>
            <optimize_move_to_prewhere>1</optimize_move_to_prewhere>
            <max_query_size>262144000</max_query_size>
            <connect_timeout_with_failover_ms>5000</connect_timeout_with_failover_ms>
            <distributed_directory_monitor_batch_inserts>1</distributed_directory_monitor_batch_inserts>
            <distributed_product_mode>global</distributed_product_mode>
            <join_use_nulls>1</join_use_nulls>
            <prefer_global_in_and_join>1</prefer_global_in_and_join>
            <readonly>0</readonly>
            <distributed_aggregation_memory_efficient>1</distributed_aggregation_memory_efficient>
            <parallel_view_processing>1</parallel_view_processing>
            <use_uncompressed_cache>1</use_uncompressed_cache>
        </default>
    </profiles>

    <users>
        <default>
            <password></password>
            <networks>
                <ip>::1</ip>
                <ip>127.0.0.1</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
        <myscaledb>
            <password>myscaledb</password>
            <networks>
                <ip>0.0.0.0/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </myscaledb>
    </users>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
